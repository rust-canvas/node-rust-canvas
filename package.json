{
  "name": "node-rust-canvas",
  "version": "0.0.0",
  "description": "rust-canvas NodeJS binding",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "shx rm -rf lib && tsc -p src/tsconfig.json",
    "build:native": "RUST_BACKTRACE=1 neon build",
    "test": "tslint -c tslint.json -p tsconfig.json",
    "watch": "tsc -p src/tsconfig.json --watch --pretty --diagnostics"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rust-canvas/node-rust-canvas.git"
  },
  "keywords": [
    "Rust",
    "Canvas",
    "node-canvas",
    "neon-binding"
  ],
  "author": "lynweklm@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rust-canvas/node-rust-canvas/issues"
  },
  "homepage": "https://github.com/rust-canvas/node-rust-canvas#readme",
  "devDependencies": {
    "@types/node": "^9.4.6",
    "canvas": "^1.6.9",
    "lodash": "^4.17.5",
    "microtime": "^2.1.7",
    "neon-cli": "^0.1.22",
    "readdir-enhanced": "^2.2.0",
    "sharp": "^0.19.1",
    "shx": "^0.2.2",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^5.0.0",
    "typescript": "^2.7.2"
  }
}
